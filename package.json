{
  "name": "claude-commands",
  "version": "1.0.0",
  "description": "Modular slash commands system for Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "list": "tsx src/cli.ts list",
    "run": "tsx src/cli.ts run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "claude",
    "commands",
    "cli",
    "slash-commands"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "tsx": "^4.7.0",
    "glob": "^10.3.0"
  }
}
