{
  "permissions": {
    "allowedTools": [
      "Read",
      "Write",
      "Edit",
      "Bash",
      "Glob",
      "Grep"
    ],
    "deniedTools": []
  },
  "hooks": {
    "postToolCall": [
      {
        "matcher": {},
        "hooks": [
          {
            "type": "command",
            "command": "./hooks/post-tool.sh"
          }
        ]
      }
    ]
  },
  "commands": {
    "autoSuggest": true,
    "selfInvoke": {
      "enabled": true,
      "confirmBeforeRun": true
    },
    "registry": {
      "typescript": "commands/ts",
      "python": "commands/py"
    }
  },
  "project": {
    "name": "claude-commands",
    "type": "cli-tool",
    "languages": ["typescript", "python"],
    "packageManager": "npm"
  }
}
